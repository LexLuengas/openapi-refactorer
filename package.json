    "swagger",
    "refactor",
    "refactorer",
    "refactoring",
    "split",
    "join"
  ],
  "main": "build/src/main.js",
  "bin": {
    "openapi-refactorer": "build/src/main.js"
  },
  "scripts": {
    "test": "jest",
    "check": "gts check",
    "clean": "gts clean",
